<ui>
    <model app="product" name="product">
        <view type="list">
            <ref>
                <actions>
                    <action>
                        <group name="main"  refType = "main"></group>
                    </action>
                    <action>
                        <group name="opAction"  refType = "main"></group>
                    </action>
                </actions>
            </ref>
            <field name="id"></field>
            <field name="name" type="static"></field>
            <field name="cost" type="static"></field>
            <field name="price" type="static"></field>
        </view>
        <view type="create">
            <ref>
                <actions>
                    <action>
                        <group name="main"  refType = "main"></group>
                    </action>
                </actions>
                <menus>
                </menus>
                <views>
                    <view type="list" ownerField="attributeMap" model="productAttributeValueMap" refType="sub|main">
                    </view>
                    <view type="create" ownerField="skuPattern" model="productSKUPattern" refType="sub|main">
                    </view>
                </views>
            </ref>
            <field name="name" type="singleLineText" style="head"></field>
            <field name="cost" type="singleLineText"></field>
            <field name="price" type="multiLineText"></field>
            <field name="uom.name" type="many2OneDataSetSelect"></field>
            <field name="attributeMap" style="relation"></field>
            <field name="skuPattern" style="relation"></field>
        </view>
        <view type="edit">
