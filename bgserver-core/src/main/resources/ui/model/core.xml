    <model name="partner" app="core">
        <view type="list">
            <ref>
                <actions>
                        <action>
                            <group name="main"></group>
                        </action>
                        <action>
                            <group name="opAction"></group>
                        </action>
                </actions>
                <menus>

                </menus>
                <views>
                    <view type="searchBox" refType="embedded"></view>
                </views>
            </ref>
            <field name="id"></field>
            <field name="userName"></field>
            <field name="name"></field>
            <field name="partnerRoles.name"></field>
            <field name="nickName"></field>
            <field name="mobile"></field>
        </view>

        <view type="searchBox">
            <field name="userName" type="criteriaStringEqual"></field>
            <field name="name" type="criteriaStringEqual"></field>
            <field name="nickName" type="criteriaStringEqual"></field>
            <field name="mobile" type="criteriaMobileEqual"></field>
            <field name="email" type="criteriaStringILike"></field>
        </view>

        <view type="create">
            <ref>
                <actions>
                    <action>
                        <group name="main"  refType = "main"></group>
                    </action>
                </actions>
                <menus>
                </menus>
                <views>
                </views>
            </ref>
            <field name="userName" style="head" type="singleLineText"></field>
            <field name="password" style="head" type="singleLineText"></field>
            <field name="name" style="subHead" type="singleLineText"></field>
            <field name="nickName" style="subHead" type="singleLineText"></field>
            <field name="birthday" type="date"></field>
            <field name="mobile" type="mobile"></field>
            <field name="telephone" type="telephone"></field>
            <field name="email" type="email"></field>
            <field name="partnerRoles.name" type="many2OneDataSetSelect"></field>
            <field name="setCurrentCorpAsDefault" type="singleCheckbox">
                <meta>
                    <![CDATA[
                    {
                     "label":"设定当前公司为默认"
                    }
                  ]]>
                </meta>
            </field>
            <field name="userTitle" colSpan="2" type="multiLineText"></field>
            <field name="userComment" colSpan="2" type="multiLineText"></field>
            <field name="label1"  title="测试标签1" style="label" type="iconLabel" icon="/icon/res/icon_label.svg"></field>
            <field name="label2"  title="测试标签2" style="label" type="iconLabel" icon="/icon/res/icon_label.svg"></field>
            <field name="label3"  title="测试标签3" style="label" type="iconLabel" icon="/icon/res/icon_label.svg"></field>
        </view>

        <view type="edit">
            <ref>
                <actions>
                    <action>
                        <group name="main"></group>
                    </action>
                </actions>
            </ref>
            <field name="userName" style="head" type="singleLineText"></field>
            <field name="password" style="head" type="singleLineText"></field>
            <field name="name" style="subHead" type="singleLineText"></field>
            <field name="nickName" style="subHead" type="singleLineText"></field>
            <field name="birthday" type="date"></field>
            <field name="mobile" type="mobile"></field>
            <field name="partnerRoles.name" type="many2OneDataSetSelect"></field>
            <field name="setCurrentCorpAsDefault" type="singleCheckbox">
                <meta>
                    <![CDATA[
                    {
                     "label":"设定当前公司为默认"
                    }
                  ]]>
                </meta>
            </field>
            <field name="telephone" type="telephone"></field>
            <field name="email" type="email"></field>
            <field name="userTitle" colSpan="2" type="multiLineText"></field>
            <field name="userComment" colSpan="2" type="multiLineText"></field>
        </view>
        <view type="detail">
            <ref>
                <actions>
                    <action>
                        <group name="main"  refType = "main"></group>
                    </action>
                </actions>
            </ref>
            <field name="userName" style="head" type="static"></field>
            <field name="name" style="subHead" type="static"></field>
            <field name="nickName" style="subHead" type="static"></field>
            <field name="birthday" type="static"></field>
            <field name="mobile" type="static"></field>
            <field name="telephone" type="static"></field>
            <field name="email" type="static"></field>
            <field name="userTitle" colSpan="2" type="static"></field>
            <field name="userComment" colSpan="2" type="static"></field>
        </view>

    </model>

    <model name="partnerRole" app="core">
        <view type="list">
            <ref>
                <actions>
                    <action>
                        <group name="main" refType="main|singleSelection|sub"></group>
                    </action>
                    <action>
                        <group name="opAction" refType="main"></group>
                    </action>
                    <action>
                        <group name="selSingleItemAction" refType="singleSelection"></group>
                    </action>
                </actions>
                <menus>
                </menus>
                <views>
                </views>
            </ref>
            <field name="id"></field>
            <field name="name"></field>
            <field name="isSuper">
            </field>
            <field name="accessControlRule">
            </field>
        </view>

        <view type="create">
            <ref>
                <actions>
                    <action>
                        <group name="main"  refType = "main"></group>
                    </action>
                </actions>
                <menus>
                </menus>
                <views>
                    <view fieldName="partners" refType="main|sub" type="list"></view>
                    <view fieldName="apps" refType="main|sub" type="list"></view>
                </views>
            </ref>
            <field name="name" style="head" type="singleLineText"></field>
            <field name="isSuper" style="subHead" type="singleCheckbox">
                <meta>
                    <![CDATA[
                    {
                     "label":"系统超级管理员"
                    }
                  ]]>
                </meta>
            </field>
            <field name="accessControlRule">
            </field>
            <field name="partners" style="relation"></field>
            <field name="apps" style="relation"></field>
            <field name="label1"  title="测试标签1" style="label" type="iconLabel" icon="/icon/res/icon_label.svg"></field>
            <field name="label2"  title="测试标签2" style="label" type="iconLabel" icon="/icon/res/icon_label.svg"></field>
            <field name="label3"  title="测试标签3" style="label" type="iconLabel" icon="/icon/res/icon_label.svg"></field>
        </view>

        <view type="edit">
            <ref>
                <views>
                    <view fieldName="partners" refType="main|sub" type="list"></view>
                    <view fieldName="apps" refType="main|sub" type="list"></view>
                </views>
                <actions>
                    <group name="main"  refType = "main"></group>
                </actions>
                <menus>

                </menus>
            </ref>
            <field name="name"></field>
            <field name="isSuper"></field>
            <field name="accessControlRule">
            </field>
            <field name="partners" style="relation"></field>
            <field name="apps" style="relation"></field>
        </view>
        <view type="detail">
            <ref>
                <views>
                    <view fieldName="partners" refType="main|sub" type="list"></view>
                    <view fieldName="apps" refType="main|sub" type="list"></view>
                </views>
                <actions>
                    <group name="main"  refType = "main"></group>
                </actions>
                <menus>

                </menus>
            </ref>
            <field name="name"></field>
            <field name="isSuper"></field>
            <field name="accessControlRule">
            </field>
            <field name="partners" style="relation"></field>
            <field name="apps" style="relation"></field>
        </view>
    </model>


</ui>
