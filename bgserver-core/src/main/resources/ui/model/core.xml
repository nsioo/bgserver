    <model name="corpPartnerRel" app="core">
        <view type="list">
            <field name="isDefaultCorp" type="singleCheckbox">
                <meta>
                    <![CDATA[
                    {
                     "label":"默认公司"
                    }
                  ]]>
                </meta>
            </field>
            <field name="partner" >
