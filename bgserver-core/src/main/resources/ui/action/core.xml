<ui>
    <action viewType="create" app="*" model="*">
        <group name="main">
            <trigger title="保存" name="doCreate"></trigger>
        </group>
    </action>

    <action viewType="edit" app="*" model="*">
        <group name="main">
            <trigger title="保存" name="doSave"></trigger>
        </group>
    </action>

    <action viewType="list" app="*" model="*">

        <group name="main">
            <trigger title="添加" name="toAdd"></trigger>
        </group>


        <group name="opAction">
            <trigger title="详情" name="toDetail" viewType="detail"></trigger>
            <trigger title="删除" name="doDelete"></trigger>
        </group>

        <group name="selSingleItemAction">
            <trigger title="选定" name="doSelSingleItem"></trigger>
        </group>

    </action>

    <action viewType="detail" app="*" model="*">
        <group name="main">
            <trigger title="编辑" name="toEdit"></trigger>
        </group>
    </action>

    <action viewType="modelAction" app="*" model="*">
        <group name="main">
            <trigger title="确定" name="doSave"></trigger>
        </group>
    </action>

</ui>